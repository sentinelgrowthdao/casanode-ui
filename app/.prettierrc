{
  "$schema": "https://json.schemastore.org/prettierrc",
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "bracketSpacing": true,
  "overrides": [
    {
      "files": ["*.vue", "*.ts", "*.tsx", "*.js", "*.jsx"],
      "options": {
        "parser": "babel-ts"
      }
    }
  ]
}
